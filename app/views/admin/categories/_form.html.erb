<%= form_for [:admin, @category] do |f| %>
 <%= render 'shared/error-message', object: f.object %> <!--error-messageからファイルを持ってくる。-->
    <div class="form-group">
      <%= f.label :title %>
      <%= f.text_field :title, class:"form-control" %>      
    </div>
    <div class="form-group">
      <%= f.label :description %>
      <%= f.text_field :description, class:"form-control" %>     
    </div>
    <p></p>
    <div class="form-group">
      <%= f.submit msg,class:"btn btn-primary", class:"form-control" %>
    </div>

<% end %>